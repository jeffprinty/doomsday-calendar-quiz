{
  "name": "doomsday-calendar-quiz",
  "version": "1.0.1",
  "author": {
    "name": "Jeff Printy",
    "email": "jeff@printy.us",
    "url": "https://drwicked.com"
  },
  "private": true,
  "homepage": "https://jeffprinty.github.io/doomsday-calendar-quiz/",
  "repository": {
    "type": "git",
    "url": "https://github.com/doinel1a/vite-react-ts-starter"
  },
  "type": "module",
  "lint-staged": {
    "*": [
      "echo 'pooopy'",
      "npx eslint --ext ts --ext tsx ./src --fix"
    ]
  },
  "scripts": {
    "prepare": "husky",
    "postinstall": "playwright install",
    "clean:dist": "npx rimraf dist",
    "clean:report": "npx rimraf playwright-report",
    "test:chrome": "npx playwright test --headed --project=chromium",
    "test:firefox": "npx playwright test --headed --project=firefox",
    "test:safari": "npx playwright test --headed --project=webkit",
    "test": "vitest",
    "lint": "npx eslint --ext ts --ext tsx ./src",
    "lint:fix": "npx eslint --ext ts --ext tsx ./src --fix",
    "dev": "vite",
    "build": "tsc && vite build && cp dist/index.html dist/404.html",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "clsx": "^2.1.1",
    "luxon": "^3.6.1",
    "random-int": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-minimal-pie-chart": "^9.1.1",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.10.2",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@playwright/test": "^1.47.2",
    "@tailwindcss/typography": "^0.5.16",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "sass": "^1.89.2",
    "tailwindcss": "^3.4.12",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
